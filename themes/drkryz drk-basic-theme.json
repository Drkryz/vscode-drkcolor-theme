{
    "name": "vscode-drkcolor-theme",
    "type": "dark",
    "colors": {
        "editor.background": "#272626",
        "editor.foreground": "#fdf7f7",
        "activityBarBadge.background": "#0099ff",
        "sideBarTitle.foreground": "#1d1d1d"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "Comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#eeff00"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#42c3ff"
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#4964df"
            }
        }
    ]
}