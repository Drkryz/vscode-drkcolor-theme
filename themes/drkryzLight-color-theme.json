{
  "colors": {
    "editor.background": "#ffffff",
    "sideBar.background": "#ffffff",
    "activityBar.background": "#ffffff",
    "activityBar.activeBackground": "#ffffff",
    "activityBar.activeBorder": "#428eff",
    "activityBar.foreground": "#444",
    "activityBar.activeFocusBorder": "#ccc",
    "activityBar.border": "#ffffff",
    "activityBar.inactiveForeground": "#428eff",
    "breadcrumb.background": "#ffffff",
    "editor.foreground": "#444",
    "sideBarSectionHeader.background": "#ffff",
    "sideBar.foreground": "#444",
    "sideBarTitle.foreground": "#444",
    "tab.activeBackground": "#ffffff",
    "tab.inactiveBackground": "#cecece",
    "tab.activeForeground": "#444",
    "tab.inactiveForeground": "#444",
    "statusBar.background": "#ffffff",
    "statusBar.foreground": "#444",
    "statusBar.debuggingBackground": "#428eff",
    "statusBar.debuggingForeground": "#fff"
  },

  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.block",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#ff0000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#6f00ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions Param, Properties",
      "scope": [
        "support.type.property-name",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#e96b03",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Typings",
      "scope": [
        "entity.name.method",
        "entity",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#0084ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "c",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#c701a6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "ClassName", 
      "scope": [
        "entity.other.inherited-class",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#0055a5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "keyword, Import, export, Async, Await",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#ff00d4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": [
        "keyword.operator",
        "keyword"
      ],
      "settings": {
        "foreground": "#FF0000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword Control, Export, await",
      "scope": [
        "keyword.control",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#877eff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "keyword",
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#00a7da",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Special Method, Variable",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#FF0000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Modifiers",
      "scope": [
        "storage.modifier",
        "storage.type"
      ],
      "settings": {
        "foreground": "#017aca",
        "fontStyle": "bold"
      }
    },
    {
      "name": "M",
      "scope": [
        "meta.tag",
        "support"
      ],
      "settings": {
        "foreground": "#ff009d",
        "fontStyle": "bold"
      }
    },
    {
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#5582ff"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
