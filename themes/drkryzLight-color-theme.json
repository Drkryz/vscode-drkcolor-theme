    "tokenColors": [
      {
        "name": "Comment",
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#000000d8"
        }
      },
      {
        "name": "Variables",
        "scope": [
          "variable",
          "string constant.other.placeholder"
        ],
        "settings": {
          "foreground": "#1f00ce"
        }
      },
      {
        "name": "Colors",
        "scope": [
          "constant.other.color"
        ],
        "settings": {
          "foreground": "#0053c0"
        }
      },
      {
        "name": "Invalid",
        "scope": [
          "invalid",
          "invalid.illegal"
        ],
        "settings": {
          "foreground": "#ff7300"
        }
      },
      {
        "name": "Keyword, Storage",
        "scope": [
          "keyword",
          "storage.modifier"
        ],
        "settings": {
          "foreground": "#006eff"
        }
      },
