				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "entity.name.method.js",
